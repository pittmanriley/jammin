{
  "expo": {
    "name": "jammin",
    "slug": "jammin",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "jammin",
    "owner": "pittmanriley",
    "plugins": ["expo-secure-store", "expo-web-browser"],
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "10",
      "bundleIdentifier": "com.jammin.app",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["jammin"],
            "CFBundleURLName": "com.jammin.app"
          }
        ]
      },
      "icon": "./assets/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jammin.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "jammin",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "spotifyClientId": "21d17026bbe0430697c7be4554663b36",
      "eas": {
        "projectId": "6cd3d7f6-401e-457d-a445-9e03e988aefe"
      }
    }
  }
}
